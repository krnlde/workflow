- var uniqueId = 0;
mixin navbar(type)
  - uniqueId++;
  .navbar(class="navbar-#{type || 'default'}")&attributes(attributes)
    .container
      .navbar-header
        button.navbar-toggle.collapsed(type="button" data-toggle="collapse" data-target="#navbar-#{uniqueId}" aria-expanded="false")
          span.sr-only Toggle navigation
          span.icon-bar
          span.icon-bar
          span.icon-bar
        a.navbar-brand(href="#") Brand #{uniqueId}

      .navbar-collapse.collapse(id="#navbar-#{uniqueId}")
        block

mixin navbar-default-content
  ul.nav.navbar-nav
    li
      a(href="") Lorem ipsum dolor.
    li
      a(href="") Eligendi, quis, reprehenderit.
    li
      a(href="") Quisquam, numquam doloremque.
    li
      a(href="") Exercitationem, nisi, ratione?
    li
      a(href="") Sed, consequatur minima?